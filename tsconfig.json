{
	"compilerOptions": {
		"outDir": "./lib",
		"module": "CommonJS",
		"allowJs": true,
		"target": "es5",
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"noEmitOnError": true,
		"moduleResolution": "Node",
		"skipLibCheck": true
	},
	"include": [
		"./src/**/*.ts"
	]
}